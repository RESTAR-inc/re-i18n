{
    "defaultLocale": "ja",
    "appType": "vue",
    "pattern": "src/{app,lib}/**/*.{ts,vue}",
    "componentName": "ReI18n",
    "locales": [
        "ja",
        "en"
    ],
    "json": {
        "outDir": "./out/json"
    },
    "csv": {
        "outDir": "./out/csv",
        "inputDir": "./out/csv",
        "delimiter": ","
    },
    "xls": {
        "outDir": "./out/xls",
        "inputDir": "./out/xls"
    },
    "generate": {
        "localeLocatorPath": "./src/i18n/useLocateLocator",
        "formatterPath": "./src/i18n/formatter"
    }
}